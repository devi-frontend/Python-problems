a=int(input())
b=a//2000
c=a-(2000*b)

d=c//500
e=c-(500*d)

f=e//200
g=e-(200*f)

h=g//50
i=g-(50*h)

j=i//20
k=i-(20*j)

L=k//5
m=k-(5*L)

n=m//2
o=m-(2*n)

p=o//1 
q=o-(1*p)

print("2000:"+str(b)+" "+
      "500:"+str(d)+" "+
      "200:"+str(f)+" "+
      "50:"+str(h)+" "+
      "20:"+str(j)+" "+
      "5:"+str(L)+" "+
      "2:"+str(n)+" "+
      "1:"+str(p))
